{
    "Window": {
        "_attributes": {
            "x:Class": "WpfApplication3.MainWindow",
            "xmlns": "http://schemas.microsoft.com/winfx/2006/xaml/presentation",
            "xmlns:x": "http://schemas.microsoft.com/winfx/2006/xaml",
            "Title": "MainWindow",
            "Height": "750",
            "Width": "300"
        },
        "DockPanel": {
            "_attributes": {
                "LastChildFill": "True"
            },
            "Label": {
                "_attributes": {
                    "DockPanel.Dock": "Top",
                    "FontSize": "26",
                    "FontWeight": "Black",
                    "FontStyle": "Normal"
                },
                "_text": "Welcome to XAML-UI Migration Example"
            },
            "StackPanel": [
                {
                    "_attributes": {
                        "DockPanel.Dock": "Left",
                        "Margin": "10",
                        "HorizontalAlignment": "Left"
                    },
                    "Label": {
                        "_attributes": {
                            "Content": "Password",
                            "HorizontalAlignment": "Left",
                            "Margin": "5,5,0,0",
                            "Width": "60"
                        }
                    },
                    "PasswordBox": {
                        "_attributes": {
                            "Name": "pwBox",
                            "Height": "15",
                            "Width": "100",
                            "MaxLength": "8",
                            "PasswordChar": "8",
                            "HorizontalAlignment": "Left"
                        }
                    },
                    "TextBlock": {
                        "_attributes": {
                            "Text": "Gender:",
                            "Margin": "5",
                            "Background": "Beige"
                        }
                    },
                    "RadioButton": [
                        {
                            "_attributes": {
                                "Name": "male",
                                "Margin": "5",
                                "Checked": "HandleCheck",
                                "GroupName": "Gender",
                                "Content": "Male"
                            }
                        },
                        {
                            "_attributes": {
                                "Name": "female",
                                "Margin": "5",
                                "Checked": "HandleCheck",
                                "GroupName": "Gender",
                                "Content": "Female"
                            }
                        }
                    ],
                    "CheckBox": {
                        "_attributes": {
                            "Name": "Age",
                            "Content": "Age more than 18?",
                            "Checked": "HandleCheck",
                            "Unchecked": "HandleUnchecked",
                            "Margin": "5"
                        }
                    },
                    "Button": {
                        "_attributes": {
                            "Name": "button1",
                            "Content": "Hover",
                            "Click": "OnClick1",
                            "Margin": "5",
                            "Width": "150",
                            "HorizontalAlignment": "Center",
                            "Foreground": "Red"
                        }
                    }
                },
                {
                    "_attributes": {
                        "Margin": "10"
                    },
                    "Label": {
                        "_attributes": {
                            "FontWeight": "Bold"
                        },
                        "_text": "Are you ready?"
                    },
                    "RadioButton": [
                        {
                            "_attributes": {
                                "GroupName": "ready"
                            },
                            "_text": "Yes"
                        },
                        {
                            "_attributes": {
                                "GroupName": "ready"
                            },
                            "_text": "No"
                        },
                        {
                            "_attributes": {
                                "GroupName": "ready",
                                "IsChecked": "True"
                            },
                            "_text": "Maybe"
                        }
                    ],
                    "ListBox": {
                        "_attributes": {
                            "Margin": "10",
                            "FontSize": "10"
                        },
                        "ListBoxItem": [
                            {
                                "_attributes": {
                                    "Background": "Aqua"
                                },
                                "_text": "Latte Macchiato"
                            },
                            {
                                "_attributes": {
                                    "Background": "Aqua"
                                },
                                "_text": "Mocha"
                            },
                            {
                                "_attributes": {
                                    "Background": "Aqua"
                                },
                                "_text": "Latte"
                            }
                        ]
                    }
                }
            ],
            "Image": {
                "_attributes": {
                    "DockPanel.Dock": "Right",
                    "Margin": "10",
                    "Source": ""
                }
            }
        }
    }
}